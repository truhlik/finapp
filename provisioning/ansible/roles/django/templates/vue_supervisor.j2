
[program:{{ vue_project_name }}]
directory = {{ vue_project_path }}
command = {{ vue_project_path }}/node_modules/nuxt/bin/nuxt.js start --prefix {{ vue_project_path }}/
user = {{ user | default(ansible_ssh_user) }}
stdout_logfile = {{ vue_project_path }}/log/npm_supervisor.log
redirect_stderr = true
environment=LANG=cs_CZ.UTF-8,LC_ALL=cs_CZ.UTF-8
autostart = true
autorestart = true
